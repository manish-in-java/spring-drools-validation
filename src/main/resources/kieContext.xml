<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:kie="http://drools.org/schema/kie-spring"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://drools.org/schema/kie-spring http://drools.org/schema/kie-spring.xsd">
  <bean class="org.kie.spring.annotations.KModuleAnnotationPostProcessor" id="kiePostProcessor"/>

  <kie:kmodule id="creditRuleModule">
    <kie:kbase name="creditKnowledgeBase" packages="rule">
      <kie:ksession name="creditKnowledgeBase" type="stateless"/>
    </kie:kbase>
  </kie:kmodule>
</beans>
